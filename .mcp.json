{
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--project-ref=ttlyfhkrsjjrzxiagzpb"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
      },
      "autoapprove": ["execute_sql", "get_database_info", "list_tables", "query_table", "insert_rows", "update_rows", "delete_rows"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "autoapprove": [
        "navigate_page",
        "take_snapshot",
        "take_screenshot",
        "click",
        "fill",
        "fill_form",
        "press_key",
        "hover",
        "drag",
        "evaluate_script",
        "get_console_message",
        "list_console_messages",
        "get_network_request",
        "list_network_requests",
        "list_pages",
        "select_page",
        "new_page",
        "close_page",
        "resize_page",
        "emulate",
        "wait_for",
        "upload_file",
        "handle_dialog",
        "performance_start_trace",
        "performance_stop_trace",
        "performance_analyze_insight"
      ]
    },
    "airtable": {
      "command": "npx",
      "args": [
        "-y",
        "airtable-mcp-server"
      ],
      "env": {
        "AIRTABLE_API_KEY": "${AIRTABLE_API_KEY}"
      }
    }
  }
}
